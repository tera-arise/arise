{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "WriteTo": {
      "Async": {
        "Args": {
          "configure": {
            "Console": {
              "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
              }
            }
          }
        }
      }
    }
  },
  "Storage": {
    "ConnectionString": "Host=localhost; Username=arise; Password=arise; Database=arise; Include Error Detail=true"
  },
  "Kestrel": {
    "Endpoints": {
      "Web": {
        "Url": "https://localhost:5000"
      }
    }
  },
  "World": {
    "Endpoints": [
      "127.0.0.1:7801",
      "::1:7801"
    ]
  }
}
