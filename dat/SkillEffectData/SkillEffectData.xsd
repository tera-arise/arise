            <xsd:element name="Normal" type="SkillEffectData_SkillEffect_AfterImage_Normal" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element name="Stage" type="SkillEffectData_SkillEffect_AfterImage_Stage" minOccurs="0" />
        </xsd:sequence>

